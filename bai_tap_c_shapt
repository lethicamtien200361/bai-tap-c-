using System;


namespace BAI_THUC_HANH_SO_3._2_IF_ELSE
{
    internal class Program
    {
       
        ///Vi·∫øt ch∆∞∆°ng tr√¨nh cho ph√©p nh·∫≠p v√†o m·ªôt s·ªë nguy√™n n. H√£y cho bi·∫øt s·ªë ƒë√≥ l√† s·ªë ch·∫µn hay so le 
         static void so_chan_hay_so_le()
        {
            lamlai:
            Console.Write(" nhap so nguyen ");
            // nhap dua lieu tu ban phiem kieu int
            int so_nguyen = int.Parse(Console.ReadLine());
            if ( so_nguyen == 0)
            {
                goto lamlai;
            }
            if( so_nguyen %2 == 0)
            {
                Console.WriteLine("EVEN");
            }
            else
            {
                Console.WriteLine("ODD");
            }
        }

        
        /// Vi·∫øt ch∆∞∆°ng tr√¨nh nh·∫≠p v√†o m·ªôt s·ªë nguy√™n n v√† cho bi·∫øt s·ªë ƒë√≥ √¢m, d∆∞∆°ng hay kh√¥ng √¢m  kh√¥ng d∆∞∆°ng.
        static void am_duong_khong_am_khong_duong()
        {
            Console.Write(" nhap so nguyen ");
            // nhap dua lieu tu ban phiem kieu int
            int n = int.Parse(Console.ReadLine());
            if( n == 0)
            {
                Console.WriteLine("UNSIGNED");
            }
            else if( n > 0)
            {
                Console.WriteLine("POSITIVE");
            }
            else
            {
                Console.WriteLine("NEGATIVE");
            }
        }


        /// Nh·∫≠p v√†o hai s·ªë nguy√™n a, b. H√£y so s√°nh v√† ƒë∆∞a ra k·∫øt lu·∫≠n xem hai s·ªë b·∫±ng nhau hay 
        //kh√°c nhau.N·∫øu kh√°c nhau th√¨ ƒë·ªô ch√™nh l·ªách l√† bao nhi√™u ƒë∆°n v·ªã t√≠nh theo tr·ªã tuy·ªát ƒë·ªëi hi·ªáu hai
        //s·ªë.
        static void bang_nhau_hon_nhau()
        {
            Console.Write(" nhap so a "); int a = int.Parse(Console.ReadLine());
            Console.Write(" nhap so b "); int b = int.Parse(Console.ReadLine());
            if(a == b)
            {
                Console.WriteLine("EQUAL");
            }
            else
            {
                int do_let = Math.Abs((a - b));
                Console.Write("DIFFERENT ");
                Console.WriteLine(do_let);
            }
        }

        /// <summary>
        /// Nh·∫≠p v√†o ba s·ªë nguy√™n a, b, c. H√£y t√¨m gi√° tr·ªã l·ªõn nh·∫•t trong ba s·ªë ƒë√≥. N·∫øu 3 s·ªë b·∫±ng 
        ///nhau th√¨ kh√¥ng c√≥ s·ªë l·ªõn nh·∫•t.
        /// </summary>
        static void tim_max()
        {
            Console.Write(" nhap a= "); int a = int.Parse(Console.ReadLine());
            Console.Write(" nhap b= "); int b = int.Parse(Console.ReadLine());
            Console.Write(" nhap c= "); int c = int.Parse(Console.ReadLine());
            if( (a>b) && (a > c))
            {
                Console.WriteLine(a);
            }else if( (b>c) && (b > a))
            {
                Console.WriteLine(b);
            }
            else if( (c>a) && (c > b))
            {
                Console.WriteLine(c);
            }else
            {
                Console.WriteLine("NO RESULT");
            }
        }

        //  Nh·∫≠p v√†o ba s·ªë nguy√™n a, b, c. H√£y t√¨m gi√° tr·ªã nh·ªè nh·∫•t trong ba s·ªë ƒë√≥. N·∫øu 3 s·ªë b·∫±ng nhau th√¨ kh√¥ng c√≥ s·ªë nh·ªè nh·∫•t.
        static void tim_min()
        {
            Console.Write(" nhap a= "); int a = int.Parse(Console.ReadLine());
            Console.Write(" nhap b= "); int b = int.Parse(Console.ReadLine());
            Console.Write(" nhap c= "); int c = int.Parse(Console.ReadLine());
            if( (a<b) && (a < c))
            {
                Console.WriteLine(a);
            }
            else if( (b<a)&& (b < c))
            {
                Console.WriteLine(b);
            }
            else if((c<a) && (c < b))
            {
                Console.WriteLine(c);
            }
            else
            {
                Console.WriteLine("NO RESULT");
            }
        }

        // B√†i 6. Gi·∫£i bi·ªán lu·∫≠n ph∆∞∆°ng tr√¨nh b·∫≠c nh·∫•t ax + b = 0.
        static void giai_phuong_tring_bat_nhat()
        {
            Console.Write("nhap a=  "); int a = int.Parse(Console.ReadLine());
            Console.Write("nhap b=  "); int b = int.Parse(Console.ReadLine());
            Console.WriteLine(" phuong trinh co dang " + a+"x + "+b+"=0");
            if( a==0 && b == 0)
            {
                Console.WriteLine("COUNTERLESS SOLUTION");
            }
            else if( (a==0) && (b != 0))
            {
                Console.WriteLine("NO SOLUTION");
            }
            else
            {
             float nghiem_phuong_trinh = (float)-b / a;
            Console.WriteLine( nghiem_phuong_trinh);
            }

        }

        //B√†i 7. Gi·∫£i bi·ªán lu·∫≠n ph∆∞∆°ng tr√¨nh b·∫≠c 2: ùëéùë•2+ ùëèùë• + ùëê = 0.
        static void bien_luan_phuong_tring_ba_hai()
        {
            Console.Write(" nhap a= "); int a = int.Parse(Console.ReadLine());
            Console.Write(" nhap b= "); int b = int.Parse(Console.ReadLine());
            Console.Write(" nhap c= "); int c = int.Parse(Console.ReadLine());
            int den_ta = b * b - 4 * a * c;
            if( (a==0) && (b==0) && (c == 0))
            {
                Console.WriteLine("COUNTERLESS SOLUTION");
            }
            else if( (a==0 ) &&(b==0) &&(c!=0))
            {
                Console.WriteLine("NO SOLUTION");
            }
            else if( den_ta == 0)
            {
                float nghiem = (float)-b / (2 * a);
                Console.WriteLine(nghiem);
            }
            else {
                float nghiem_1 = (float)(-b + Math.Sqrt(den_ta)) / (2 * a);
                float nghiem_2 = (float)(-b - Math.Sqrt(den_ta)) / (2 * a);
                Console.WriteLine(nghiem_1 +"\t" + nghiem_2);
            }
        }

        //Cho 3 s·ªë th·ª±c a, b, c. H√£y ki·ªÉm tra xem ba s·ªë ƒë√≥ c√≥ t·∫°o th√†nh 3 c·∫°nh c·ªßa m·ªôt tam gi√°c   hay kh√¥ng.
        //Ba c·∫°nh a, b, c c·ªßa m·ªôt tam gi√°c ph·∫£i th·ªèa m√£n 
        //ƒëi·ªÅu ki·ªán l√† t·ªïng hai c·∫°nh b·∫•t k√¨ lu√¥n l·ªõn h∆°n c·∫°nh c√≤n l·∫°i.T·ª©c l√† a+b>c v√† b+c>a v√† a+c>b.
        static void kiem_tra_co_phai_tao_thanh_tam_giac()
        {
            Console.Write("nhap a= ");int a= int.Parse(Console.ReadLine());
            Console.Write("nhap b= ");int b= int.Parse(Console.ReadLine());
            Console.Write("nhap c= ");int c= int.Parse(Console.ReadLine());
            if( (a>0) &&(b>0)&&(c>0)&& (((a+b)>c)||((b+c)>a) || ((a + c) > b)))
            {
                Console.WriteLine("YES");
            }
            else
            {
                Console.WriteLine("NO");
            }
        }

        //B√†i 9. Cho 3 s·ªë th·ª±c a, b, c. H√£y ki·ªÉm tra xem ba s·ªë ƒë√≥ c√≥ t·∫°o th√†nh tam gi√°c vu√¥ng hay kh√¥ng.
        static void kiem_tra_tam_giac_vuong()
        {
            Console.Write("nhap a= "); int a = int.Parse(Console.ReadLine());
            Console.Write("nhap b= "); int b = int.Parse(Console.ReadLine());
            Console.Write("nhap c= "); int c = int.Parse(Console.ReadLine());
            // muon kie tra tam giac vuong thi chung minh binh phuong canh huyen bang tong binh phuong 
            // 2 canh goc vuong 
            if( (a*a == b*b+c*c) || (b*b==a*a+c*c) ||(c * c == a * a + b * b))
            {
                Console.WriteLine("YES");
            }
            else
            {
                Console.WriteLine("NO");
            }
        }

        //B√†i 10. Nh·∫≠p v√†o ƒëi·ªÉm c·ªßa m·ªôt sinh vi√™n(s·ªë th·ª±c l·∫ª 0.25 t·ª´ 0-10) v√† h√£y cho bi·∫øt sinh vi√™n ƒë√≥ 
        //ƒë·∫°t ƒëi·ªÉm ch·ªØ t∆∞∆°ng ·ª©ng l√† bao nhi√™u
        static void diem_sinh_vien()
        {
            lamlai:
            // nhap diem co kieu float tu ban phim 
            Console.Write("nhap diem sinh vien "); float diem = float.Parse(Console.ReadLine());
            if( diem <0 || diem > 10)
            {
                goto lamlai;
            }
            if( diem > 9)
            {
                Console.WriteLine("A");
            }
            else if (diem > 7)
            {
                Console.WriteLine("B");
            }
            else if(diem > 5)
            {
                Console.WriteLine("C");
            }
            else if(diem > 4)
            {
                Console.WriteLine("D");
            }
            else
            { Console.WriteLine("F"); }
        }
        static void Main()
        {
            // bai 1
            //so_chan_hay_so_le();
            //bai 2
            //am_duong_khong_am_khong_duong();
            //bai 3 
            //bang_nhau_hon_nhau();
            // bai 4
            //tim_max();
            //bai 5
            // tim_min();
            // bai 6
            //giai_phuong_tring_bat_nhat();
            //bai 7
            // bien_luan_phuong_tring_ba_hai();
            // bai 8
            //kiem_tra_co_phai_tao_thanh_tam_giac();
            //bai 9
            //kiem_tra_tam_giac_vuong();
            //bai 10
            diem_sinh_vien();

            // dung man hinh de xem ket qua 
            Console.ReadKey();
        }

    }
}
